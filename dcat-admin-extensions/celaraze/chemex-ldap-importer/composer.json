        "chemex",
        "extension"
    ],
    "homepage": "https://github.com/celaraze/chemex-ldap-importer",
    "license": "MIT",
    "authors": [
        {
            "name": "Celaraze",
            "email": "celaraze@qq.com"
        }
    ],
    "require": {
